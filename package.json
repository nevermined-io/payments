    "eslint": "^8.56.0",
    "eslint-config-nevermined": "^0.2.0",
    "eslint-config-next": "^15.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "@types/jest": "^29.5.13",
    "@types/ws": "^8.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "0.25.13",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "socket.io-client": "4.7.5",
